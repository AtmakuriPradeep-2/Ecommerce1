    &lt;div class="chatbot-window" id="chatbot"&gt;
        &lt;div class="chatbot-header"&gt;Chat with us&lt;/div&gt;
        &lt;div class="chatbot-messages" id="chat-messages"&gt;&lt;/div&gt;
        &lt;div class="chatbot-input"&gt;
            &lt;input type="text" id="chat-input" placeholder="Type a message..."&gt;
            &lt;button onclick="sendMessage()"&gt;Send&lt;/button&gt;
        &lt;/div&gt;
    &lt;/div&gt;
    
    &lt;script&gt;
 let selectedOption = ""; // To track the user's choice

function toggleChatbot() {
    let chatbot = document.getElementById("chatbot");
    let chatMessages = document.getElementById("chat-messages");

    chatbot.style.display = chatbot.style.display === "none" ? "flex" : "none";

    // Show options only when chatbot opens and no messages exist
    if (chatbot.style.display === "flex" &amp;&amp; chatMessages.childElementCount === 0) {
        let botMessage = document.createElement("div");
        botMessage.innerHTML = "🤖 &lt;b&gt;Welcome!&lt;/b&gt;&lt;br&gt;Choose an option:&lt;br&gt;1️⃣ Track Order&lt;br&gt;2️⃣ Cancel Order&lt;br&gt;3️⃣ Browse Products&lt;br&gt;4️⃣ Contact Support";
        chatMessages.appendChild(botMessage);
    }
}

function sendMessage() {
    let input = document.getElementById("chat-input");
    let message = input.value.trim();
    if (message === "") return;

    let chatMessages = document.getElementById("chat-messages");

    // Add user message
    let userMessage = document.createElement("div");
    userMessage.innerHTML = <code>&lt;b&gt;You:&lt;/b&gt; ${message}</code>;
    chatMessages.appendChild(userMessage);
    input.value = "";

    setTimeout(() =&gt; {
        let botMessage = document.createElement("div");

        if (message.startsWith("#")) {
            let orderId = message.substring(1); // Remove "#" from input

            fetch(<code>http://localhost/Ecommerce/pages/check_order_status.php?order_id=${orderId}</code>)
                .then(response =&gt; response.text())
                .then(status =&gt; {
                    let botResponse = "";

                    if (status.trim().toLowerCase() === "canceled") {
                        botResponse = <code>⚠️ &lt;b&gt;Order Already Canceled&lt;/b&gt;&lt;br&gt;❌ Order #${orderId} has already been canceled.&lt;br&gt;🔍 Need help? &lt;a href="mailto:support@megamart.com" style="color: blue; font-weight: bold;"&gt;Contact Support&lt;/a&gt;</code>;
                    } else if (status.trim().toLowerCase() === "invalid") {
                        botResponse = <code>🚫 &lt;b&gt;Invalid Order ID&lt;/b&gt;&lt;br&gt;🔎 Please check and enter a valid order ID.</code>;
                    } else {
                        if (selectedOption === "1") { 
                            // If user selected "Track Order"
                            botResponse = <code>📦 &lt;b&gt;Track Order&lt;/b&gt;&lt;br&gt;✅ &lt;a href="http://localhost/Ecommerce/pages/trackorder.php?order_id=${orderId}" target="_blank" style="color: green; font-weight: bold;"&gt;Track Order #${orderId}&lt;/a&gt;</code>;
                        } else if (selectedOption === "2") { 
                            // If user selected "Cancel Order"
                            botResponse = <code>❌ &lt;b&gt;Cancel Order&lt;/b&gt;&lt;br&gt;🚫 &lt;a href="http://localhost/Ecommerce/pages/cancelorder.php?order_id=${orderId}" target="_blank" style="color: red; font-weight: bold;"&gt;Cancel Order #${orderId}&lt;/a&gt;</code>;
                        } else {
                            botResponse = <code>⚠️ &lt;b&gt;Invalid Request&lt;/b&gt;&lt;br&gt;📌 Please select an option first:&lt;br&gt;1️⃣ Track Order&lt;br&gt;2️⃣ Cancel Order</code>;
                        }
                    }

                    botMessage.innerHTML = botResponse;
                    chatMessages.appendChild(botMessage);
                })
                .catch(error =&gt; {
                    botMessage.innerHTML = "⚠️ &lt;b&gt;Error fetching order status.&lt;/b&gt; Please try again later.";
                    chatMessages.appendChild(botMessage);
                    console.error("Error fetching order status:", error);
                });

        } else {
            let response = "";
            switch (message) {
                case "1":
                    selectedOption = "1"; // Track Order
                    response = "🛒 &lt;b&gt;Track Order&lt;/b&gt;&lt;br&gt;📌 Enter your order ID prefixed with &lt;b&gt;'#'&lt;/b&gt; to track.&lt;br&gt;Example: &lt;b&gt;#35&lt;/b&gt;";
                    break;
                case "2":
                    selectedOption = "2"; // Cancel Order
                    response = "❌ &lt;b&gt;Cancel Order&lt;/b&gt;&lt;br&gt;🔄 Enter your order ID prefixed with &lt;b&gt;'#'&lt;/b&gt; to cancel.&lt;br&gt;Example: &lt;b&gt;#35&lt;/b&gt;";
                    break;
